a = 1
b = 2
m = -5
n = 5
x =-b/a

is_ok = n>x>m

print("Попадает:", is_ok)


# --------------
# Пример вывода:
#
# Введите a = 1
# Введите b = 2
# Введите m = -5
# Введите n = 5
# Попадает: True
