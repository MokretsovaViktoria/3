word = "объектно-ориентированный"

w1 = word[:6]  # (слово объект)
w2 = word[9:17]
w3 = word[5]+word[11]+word[-8]
w4 = word[4]+word[0]+word[5]
w5 = word[10]+word[12:15]+word[-5]

print(w1, w2, w3, w4, w5, sep="\n")


# --------------
# Пример вывода:
#
# объект
# ориентир
# тир
# кот
# рента
